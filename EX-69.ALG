algoritmo "EX-69"
var
numerodaPA, soma, decimo:inteiro
inicio
  Escreval("Digite o primeiro termo da PA")
  Leia (numerodaPA)
  decimo<- numerodaPA + (10-1)
  Para numerodaPA<- numerodaPa ate decimo faca
    Escreval(numerodaPA)
    soma<- soma + numerodaPA
  FimPara
  Escreval("Soma da PA :" , soma)

fimalgoritmo